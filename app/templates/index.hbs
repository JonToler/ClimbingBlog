
<h1>Great Climbs!</h1>
<div class="newClimb">
  {{new-climb saveClimb2="saveClimb3"}}

</div>

<ul>
  {{#each model.climbs as | climb|}}
    <div class="jumbotron">
    {{climb-tile climb=climb }}
    </div>
  {{/each}}
</ul>

<h2> All Reviews </h2>

<ul>
  {{#each model.comments as |comment|}}
    <li>by:{{comment.name}} - {{comment.rating}}  {{comment.comment}}</li>
  {{/each}}
</ul>

{{new-comment saveComment="saveComment"}}
<hr>
